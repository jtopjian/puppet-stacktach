WSGIScriptAlias / <%= @install_dir -%>/wsgi/django.wsgi
WSGIDaemonProcess stacktach user=<%= @www_user %> group=<%= @www_user %> processes=3 threads=10
Alias /static <%= @install_dir -%>/app/static
<Directory <%= @install_dir-%>/wsgi>
  Order allow,deny
  Allow from all
  <% if @htpasswd -%>
    AuthName "StackTach Access"
    AuthType Basic
    AuthUserFile <%= @htpasswd %>
    require valid-user
  <% end %>
</Directory>
